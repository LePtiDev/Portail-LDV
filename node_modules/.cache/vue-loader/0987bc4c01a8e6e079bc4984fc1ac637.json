buttons\">\n                <img src=\"./../assets/horloge.png\">\n                <router-link to=\"#\">\n                <a>Absences</a>\n                </router-link>\n            </div>\n\n            <div id=\"buttons\">\n                <img src=\"./../assets/reglage.png\">\n                <router-link to=\"./CoursView\">\n                <a> devinci online</a>\n                </router-link>\n\n            </div>\n\n            <div id=\"buttons\">\n                <img src=\"./../assets/oeil.png\">\n                <router-link to=\"#\">\n                <a>Promotion école</a>\n                </router-link>\n            </div>\n            <hr id=\"medium\">\n            \n            <p class=\"service\"> Services </p>\n\n            <div id=\"buttons\">\n                <img src=\"./../assets/partage.png\">\n                <router-link to=\"#\">\n                <a>Occupation des salles</a>\n                </router-link>\n            </div>\n\n            <div id=\"buttons\">\n                <img src=\"./../assets/avion.png\">\n                <router-link to=\"#\">\n                <a>International</a>\n                </router-link>\n            </div>\n            <hr id=\"bottom\">\n\n            <p class=\"service\"> Divers </p>\n\n            <div id=\"buttons\">\n                <img src=\"./../assets/croix.png\">\n                <router-link to=\"/Ressources\">\n                <a>Ressources</a>  \n                </router-link>              \n            </div>\n\n        <div class=\"footer\">\n        <div class=\"etudiant\">\n            <img src=\"./../assets/Puech-Antoine.jpg\" id=\"photoProfil\">\n            <p id=\"nom\">Antoine Puech</p>\n        </div>\n        <p id=\"copyright\"> ALV Online - 2020 © Léonard de Vinci </p>\n        </div>\n        </div>\n        </div>\n    </div>\n    \n</template>\n\n<script>\n\n    export default {\n        name: 'Menu',\n        props: {\n            student: String,\n        },\n        methods: {\n            animation: function () {\n                const menuBtn = document.querySelector('.menu-btn');\n                let menuOpen = false;\n                \n                 if(!menuOpen) {\n                    menuBtn.classList.toggle('open');\n                    menuOpen = true;\n                    var element = document.getElementById(\"menu\");\n                    element.classList.toggle(\"mystyle\");\n                }\n            }\n        }\n    }\n\n</script>\n\n<style scoped>\n*{\n    text-decoration: none;\n    color: #ffffff;\n}\n/* menu burger */\n.menu-btn {\n  position: absolute;\n  display: none;\n  justify-content: center;\n  align-items: center;\n  width: 80px;\n  height: 80px;\n  cursor: pointer;\n  transition: all .5s ease-in-out;\n  z-index: 10;\n  /* border: 3px solid #fff; */\n}\n.menu-btn__burger {\n  width: 35px;\n  height: 8px;\n  background: #fff;\n  border-radius: 5px;\n  box-shadow: 0 2px 5px rgba(255,101,47,.2);\n  transition: all .5s ease-in-out;\n}\n.menu-btn__burger::before {\n  transform: translateY(-16px);\n  content: '';\n  position: absolute;\n  width: 50px;\n  height:8px;\n  background: #fff;\n  border-radius: 5px;\n  box-shadow: 0 2px 5px rgba(255,101,47,.2);\n  transition: all .5s ease-in-out;\n}\n.menu-btn__burger::after {\n  transform: translateY(16px);\n  content: '';\n  position: absolute;\n  width: 30px;\n  height: 8px;\n  background: #fff;\n  border-radius: 5px;\n  box-shadow: 0 2px 5px rgba(255,101,47,.2);\n  transition: all .5s ease-in-out;\n}\n/* ANIMATION */\n.menu-btn.open .menu-btn__burger {\n  transform: translateX(-50px);\n  background: transparent;\n  box-shadow: none;\n}\n.menu-btn.open .menu-btn__burger::before {\n  transform: rotate(45deg) translate(35px, -35px);\n  width: 40px;\n  height:8px;\n}\n.menu-btn.open .menu-btn__burger::after {\n  transform: rotate(-45deg) translate(35px, 35px);\n  width: 40px;\n  height:8px;\n}\n/*/////////////////////////////////*/\n#menu {\n    width: 17vw;\n    height: 100vh;\n    background-color: #242424;\n    font-family: 'Montserrat';\n    color: #ffffff;\n    transition-duration: 600ms;\n    position: fixed;\n    top: 0;\n    left:0;\n\n}\n\n#top {\n    margin: -10px 0;\n    border-width: 0.5px;\n}\n\n#medium {\n    margin: 10px 0;\n    border-width: 0.5px;\n}\n\n#bottom {\n    border-width: 0.5px;\n}\n\n.logo {\n    text-align: center;\n}\n\n.logo img {\n    width: 75%;\n    margin-top: 20px;\n}\n\n.direction{\n    margin-top: 25px;\n    text-transform: uppercase;    \n}\n\n#button {\n    display: flex;\n    align-items: center;\n    padding: 0 10%;\n    margin: 15px 0;\n}\n\n#button a{\n    padding: 0 5px;\n    font-size: 15px;\n    font-weight: bold;\n}\n\n#button img {\n    width: 18px;\n}\n\n#buttons {\n    display: flex;\n    align-items: center;\n    padding: 0 10%;\n    height: 34px;\n}\n\n#buttons a{\n    padding: 0 5px;\n    font-size: 14px;\n    color: #ffffff;\n    text-decoration: none;\n    font-weight: bold;\n}\n\n#buttons img {\n    width: 18px;\n}\n\n.categorie{\n    margin-bottom: 5px;\n    margin-top: 25px;\n    font-size: 10px;\n    padding: 0 20%;\n    color: lightgrey;\n    text-transform: capitalize;\n}\n\n.service{\n    margin-bottom: 5px;\n    margin-top: 10px;\n    font-size: 10px;\n    padding: 0 20%;\n    color: lightgrey;\n    text-transform: capitalize;\n}\n\n.etudiant {\n    display: flex;\n    /* justify-content: center; */\n    /* margin-top: 20%; */\n    position: absolute;\n    bottom: 50px;\n    align-items: center;\n    left: 50px;\n}\n\n.etudiant #photoProfil {\n    width: 5%;\n    border-radius: 100px;\n    height: 15%;\n    margin: 0 15px;\n}\n\n.etudiant #nom {\n    padding: 0 5px;\n    font-family: 'Montserrat';\n    font-weight: bold;\n    font-size: 13px;\n    text-transform: none;\n}\n\n#copyright {\n    font-size: 12px;\n    color: #ffffff;\n    opacity: 50%;\n    text-align: center;\n    position: absolute;\n    bottom: 15px;\n    left: 30px;\n}\n/* responsive design here */\n\n@media screen and (min-width: 790px) and (max-width: 1120px) {\n    #button a{\n    font-size: 10px;\n    }\n    #buttons a{\n        font-size: 9px;\n    }\n    #copyright{\n        font-size: 9px;\n        color: #ffffff;\n        opacity: 50%;\n        text-align: center;\n        position: absolute;\n        bottom: 15px;\n        left: 14px;\n    }\n    .etudiant {\n        left: 10px;\n    }\n}\n@media screen and (min-width: 601px) and (max-width: 789px) {\n    #button a{\n    font-size: 8px;\n    }\n    #buttons a{\n        font-size: 6.5px;\n        line-height: 14px;\n    }\n    #copyright{\n        font-size: 6px;\n        color: #ffffff;\n        opacity: 50%;\n        text-align: center;\n        position: absolute;\n        bottom: 15px;\n        left: 16px;\n    }\n    .etudiant {\n        left: 10px;\n    }\n    .etudiant #nom {\n        font-size: 10px;\n    }\n    .etudiant #photoProfil {\n        margin: 0 10px;\n    }\n}\n@media screen and (min-width: 0px) and (max-width: 600px) {\n    #menu{\n        position: absolute;\n        left: -2000px;\n        width: 100%;\n        z-index: 5;\n    }\n    .direction{\n        text-align:center;\n    }\n    .mystyle{\n    width: 100% !important;\n    transition-duration: 500ms;\n    left: 0 !important;\n    transition: 700ms;\n    }\n    .menu-btn{\n        display: flex;\n    }\n    \n    .logo img {\n        width: 40%;\n    }\n    #button a{\n        font-size: 12px;\n    }\n    #buttons a{\n        font-size: 12px;\n    }\n    #button img{\n        width: 15px;\n    }\n    #buttons img{\n        width: 15px;\n    }\n    #buttons{\n        height: 30px;\n        justify-content: center;\n    }\n    #button{\n        justify-content: center;\n    }\n    #copyright{\n        font-size: 12px;\n        color: #ffffff;\n        opacity: 50%;\n        text-align: center;\n        position: absolute;\n        bottom: 15px;\n        left: 18%;\n    }\n    .etudiant {\n        left: 30%;\n    }\n    .etudiant #nom {\n        font-size: 15px;\n    }\n    .etudiant #photoProfil {\n        margin: 0 2px;\n        width: 20%;\n    }\n\n}\n\n\n</style>"]}]}