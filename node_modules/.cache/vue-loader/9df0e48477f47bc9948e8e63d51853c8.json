iv class=\"container\">\n    <div class=\"txt\">\n      <h2>EMPLOI DU TEMPS</h2>\n      <span>></span>\n      <h2>AJOUTER UN évènement</h2>\n    </div>\n    <div class=\"informations\">\n      <div class=\"row1\">\n        <div class=\"nomEvent\">\n          <h3>NOM de l'évènement</h3>\n          <input class=\"bg-dark\" type=\"text\" placeholder=\"Nom de l'évènement\" />\n        </div>\n        <div class=\"typeEvent\">\n          <h3>Type</h3>\n          <input class=\"bg-dark\" type=\"text\" placeholder=\"Type de l'évènement\" />\n        </div>\n      </div>\n      <div class=\"row2\">\n        <h3>AJOUTER DES PERSONNES</h3>\n        <input class=\"bg-dark\" type=\"text\" placeholder=\"Participants\" />\n      </div>\n      <div class=\"row3\">\n        <h3>SALLE à réserver</h3>\n        <input class=\"bg-dark\" list=\"alexisCROFOR\" placeholder=\"Numéro de salle à choisir\" />\n        <datalist id=\"alexisCROFOR\">\n          <option value=\"Alexis\"></option>\n          <option value=\"Quentin\"></option>\n          <option value=\"Maxime\"></option>\n          <option value=\"Antoine\"></option>\n          <option value=\"Bitos\"></option>\n           <option value=\"Bitos\"></option>\n        </datalist>\n      </div>\n      <div class=\"row4\">\n        <div class=\"nomEvent\">\n          <h3>Date</h3>\n          <input class=\"bg-dark\" type=\"date\" placeholder=\"JJ / MM / YY\" />\n        </div>\n        <div class=\"typeEvent\">\n          <h3>Durée</h3>\n          <input class=\"bg-dark\" type=\"texte\" placeholder=\"- - H - - | - - H - -\" />\n          <div class=\"dayComplete\">\n            <label class=\"coucou\">\n              Journée complète\n              <input type=\"checkbox\" />\n              <span class=\"checkmark\"></span>\n            </label>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"AddEvent\",\n  props: {\n    msg: String\n  }\n};\n</script>\n\n\n<style scoped>\n/* container total */\n.container {\n  height: 100vh;\n  width: 83vw;\n  background-color: #121212;\n  padding: 25px 35px 35px 35px;\n  position: relative;\n  left: 17vw;\n  top: 0;\n  z-index: 0;\n}\n\n/* configue page */\n.txt {\n  display: flex;\n  text-transform: uppercase;\n}\nh2 {\n  font-size: 20px;\n  color: white;\n  filter: opacity(0.5);\n}\n\nh3 {\n  margin: 14px 0;\n  text-transform: uppercase;\n}\nh4 {\n  text-transform: uppercase;\n}\n\nspan {\n  margin: 0 13px;\n  filter: opacity(1);\n  font-weight: 900;\n  font-size: 20px;\n}\n\n.informations {\n  width: 100%;\n  height: 90%;\n  margin-top: 40px;\n  padding: 0 50px;\n}\n\n.row1,\n.row4 {\n  display: flex;\n  justify-content: space-between;\n  width: 100%;\n}\n.nomEvent,\n.typeEvent {\n  width: 49%;\n}\n\ninput {\n  width: 100%;\n}\n\ninput#submit {\n  width: 20%;\n}\n\n.bg-dark {\n  background-color: #242424;\n  border-radius: 4px;\n  padding: 30px 40px;\n  border: none;\n  outline: none;\n  font-size: 16px;\n}\n\n.bg-dark p {\n  opacity: 0.5;\n}\n\n.row2,\n.row3,\n.row4 {\n  margin-top: 33px;\n}\n\n.row3 .row4 button {\n  border: 2px solid #ee7f01;\n  outline: none;\n  display: flex;\n  align-items: center;\n  font-size: 16px;\n  cursor: pointer;\n}\n\nform input[type=\"submit\"] {\n  margin-top: 5vh;\n  text-transform: uppercase;\n  font-weight: bold;\n  padding: 10px 15px;\n  background-color: #27ae60;\n  font-size: 18px;\n  border: none;\n  outline: none;\n  border-radius: 4px;\n  cursor: pointer;\n}\n\n.dayComplete {\n  padding-top: 10px;\n  padding-right: 10%;\n  display: flex;\n  text-align: right;\n  justify-content: flex-end;\n  align-items: center;\n}\n.dayComplete input {\n  width: 10%;\n  border: 2px solid white;\n  border-radius: 4px;\n  outline: none;\n  background-color: none;\n}\n\n.coucou{\n  display: block;\n  position: relative;\n  padding-left: 35px;\n  padding-top: 12px;\n  cursor: pointer;\n  font-size: 16px;\n  -webkit-user-select: none;\n  -moz-user-select: none;\n  -ms-user-select: none;\n  user-select: none;\n}\n\n/* Hide the browser's default checkbox */\n.coucou input {\n  position: absolute;\n  opacity: 0;\n  cursor: pointer;\n  height: 0;\n  width: 0;\n}\n\n/* Create a custom checkbox */\n.checkmark {\n  position: absolute;\n  height: 25px;\n  width: 25px;\n  border: 2px solid white;\n  border-radius: 4px;\n}\n\n/* On mouse-over, add a grey background color */\n.coucou:hover input ~ .checkmark {\n  background-color: #ccc;\n}\n\n/* When the checkbox is checked, add a blue background */\n.coucou input:checked ~ .checkmark {\n  background-color: none;\n}\n\n/* Create the checkmark/indicator (hidden when not checked) */\n.checkmark:after {\n  content: \"\";\n  position: absolute;\n  display: none;\n}\n\n/* Show the checkmark when checked */\n.coucou input:checked ~ .checkmark:after {\n  display: block;\n}\n\n/* Style the checkmark/indicator */\n.coucou .checkmark:after {\n  left: 9px;\n  top: 3px;\n  width: 5px;\n  height: 10px;\n  border: solid white;\n  border-width: 0 3px 3px 0;\n  -webkit-transform: rotate(45deg);\n  -ms-transform: rotate(45deg);\n  transform: rotate(45deg);\n}\n</style>\n"]}]}